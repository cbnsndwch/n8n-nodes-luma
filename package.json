{
  "name": "n8n-nodes-luma",
  "version": "0.1.0",
  "description": "n8n community nodes for Luma's API",
  "keywords": [
    "n8n-community-node-package",
    "n8n-nodes",
    "luma",
    "api"
  ],
  "license": "MIT",
  "homepage": "https://github.com/cbnsndwch/n8n-nodes-luma",
  "author": {
    "name": "cbnsndwch",
    "email": "8313760+cbnsndwch@users.noreply.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cbnsndwch/n8n-nodes-luma.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/LumaApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Luma/Luma.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.16.16",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "jest": "^29.5.0",
    "n8n-core": "~1.24.0",
    "n8n-workflow": "~1.24.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.1.0",
    "typescript": "~4.8.4"
  }
}