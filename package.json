{
    "name": "@cbnsndwch/n8n-nodes-luma",
    "version": "0.1.0",
    "description": "n8n community nodes for Luma's API",
    "license": "MIT",
    "homepage": "https://github.com/cbnsndwch/n8n-nodes-luma",
    "keywords": [
        "n8n-community-node-package"
    ],
    "author": {
        "name": "cbnsndwch",
        "url": "https://cbnsndwch.io"
    },
    "scripts": {
        "build": "pnpm run clean && tsc && gulp build:icons",
        "clean": "rimraf dist",
        "dev": "tsc --watch",
        "format": "prettier nodes credentials --write",
        "lint": "eslint nodes credentials package.json",
        "lintfix": "eslint nodes credentials package.json --fix",
        "prepublishOnly": "pnpm run build && pnpm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
        "test:local:setup": "tsx .n8n/scripts/setup.ts",
        "test:local:start": "tsx .n8n/scripts/start-n8n.ts",
        "test:local:link": "tsx .n8n/scripts/link-nodes.ts",
        "test:local:health": "tsx .n8n/scripts/health-check.ts",
        "test:local": "pnpm run build && pnpm run test:local:link && pnpm run test:local:start",
        "dev:local": "concurrently \"pnpm run dev\" \"tsx .n8n/scripts/watch.ts\"",
        "test:local:help": "tsx .n8n/scripts/help.ts",
        "test:local:watch": "tsx .n8n/scripts/watch.ts"
    },
    "files": [
        "dist"
    ],
    "main": "dist/index.js",
    "devDependencies": {
        "@typescript-eslint/parser": "~8.40.0",
        "concurrently": "^9.0.1",
        "eslint": "^8.57.0",
        "eslint-plugin-n8n-nodes-base": "^1.16.3",
        "gulp": "^5.0.1",
        "n8n-workflow": "*",
        "npm-check-updates": "^18.0.2",
        "prettier": "^3.6.2",
        "rimraf": "^6.0.1",
        "tsx": "^4.20.4",
        "typescript": "~5.8.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    },
    "engines": {
        "node": ">=22"
    },
    "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/LumaApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Luma/Luma.node.js",
            "dist/nodes/LumaTrigger/LumaTrigger.node.js"
        ]
    }
}